<?xml version="1.0"?>
<extension key="com.joineryhq.estatweaks" type="module">
  <file>estatweaks</file>
  <name>ESTA Tweaks</name>
  <description>Custom CiviCRM modifications for ESTA.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/com.joineryhq.estatweaks</url>
    <url desc="Documentation">https://github.com/twomice/com.joineryhq.estatweaks</url>
    <url desc="Support">https://github.com/twomice/com.joineryhq.estatweaks/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2020-05-11</releaseDate>
  <version>1.1-5.28.3</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.28.3</ver>
  </compatibility>
  <comments>Developed by Joinery for Korlon and ESTA. This extension makes use of PHP file overrides. Developer support will be needed for any upgrade of CiviCRM, to prevent misbehavior from mismatching file versions.
</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Estatweaks</namespace>
  </civix>
</extension>
